    connect unitDelay1.out1 -> cCSetValue_kmh;
    connect unitDelay.out1 -> limiterSetValue_kmh;
    connect limiter.limiter_active_bOut2 -> limiter_active_b;
    connect parkingBrake_b -> fAS_Input.parkingBrake_b1;
