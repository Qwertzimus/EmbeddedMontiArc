package fas.demo_fas_Fkt_m.lim_m.lim_Fkt_m.lim_Ena_m;

import fas.basicLibrary.*;

component Limiter_Active {
  port
	out Double vMax_kmhOut1,
	out Boolean limiter_active_bOut2,
	in Double in1;

  instance Gain gain;
  instance And logOp;
  instance Constant<Boolean>(true) trueBlock;
  
  connect in1 -> gain.in1;
  connect logOp.out1 -> limiter_active_bOut2;
  connect gain.out1 -> vMax_kmhOut1;
  connect trueBlock.out1 -> logOp.in1;
  connect trueBlock.out1 -> logOp.in2;
}
