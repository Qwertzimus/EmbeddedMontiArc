package fas.demo_fas_Fkt_m.tem_m.t_Fkt_m.cc_Set_m;

import fas.basicLibrary.*;
import fas.advancedLibrary.*;

component CC_InitialSetValue {
	port
		in (0m/s:0.28m/s:1820.39m/s) V_Vehicle_kmh,
		in Boolean trigger,
		in (0m/s:0.28m/s:1820.39m/s) DEMO_FAS_V_CCSetValue_Read,
		in Boolean Enable,
		out (0m/s:0.28m/s:1820.39m/s) DEMO_FAS_V_CCSetValue;

	instance SwitchB<Boolean> switch1, switch2;
	instance SwitchB<(0m/s:0.28m/s:1820.39m/s)> switch3, switch4;
	instance Greater<Boolean> greater1, greater2;
	instance Memory<Boolean>(false) mem1;
	instance Memory<Boolean>(true) mem2;
	instance Smaller<(0m/s:0.28m/s:1820.39m/s)> smaller1;

	connect trigger -> greater1.in1, switch1.in1;
	connect Enable -> switch1.cond, switch2.cond;
	connect DEMO_FAS_V_CCSetValue_Read -> smaller1.in1, switch3.in3, switch4.in3;
	connect V_Vehicle_kmh -> switch3.in1;
	connect 0 -> smaller1.in2;
	connect smaller1.out1 -> switch3.cond;
	connect switch3.out1 -> switch4.in1;
	connect switch1.out1 -> mem1.in1;
	connect mem1.out1 -> switch1.in3, greater1.in2;
	connect greater1.out1 -> greater2.in1;
	connect switch2.out1 -> mem2.in1;
	connect mem2.out1 -> switch2.in3, greater2.in2;
	connect greater2.out1 -> switch4.cond, switch2.in1;
	connect switch4.out1 -> DEMO_FAS_V_CCSetValue;
}
